generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Question {
  id         Int      @id @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamptz(6)
  content    String?
  level      Int?
}

model SurveyResult {
  id         Int      @id @default(autoincrement())
  content    String?
  created_at DateTime @default(now()) @db.Timestamptz(6)
}
